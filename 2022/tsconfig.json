{
  "compilerOptions": {
      "target": "es2022",
      "strict": true,
      "preserveConstEnums": true,
      "noEmit": true,
      "module": "ES2022",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node"],
      "noImplicitAny": true
  },
  "exclude": ["node_modules"]
}